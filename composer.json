{
  "name": "metamodels/attribute_article",
  "description": "Article Attribute for Metamodels",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "article",
    "nontranslatable"
  ],
  "type": "contao-bundle",
  "homepage": "http://www.hinderlingvolkart.com",
  "license": "Commercial",
  "authors": [
    {
      "name": "Ronny Binder",
      "email": "rbi@hinderlingvolkart.com",
      "homepage": "http://www.hinderlingvolkart.com/",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@hinderlingvolkart.com",
    "issues": "https://tracker.hinderlingvolkart.com/projects/contao-metamodelsattribute_article/issues"
  },
  "require": {
    "php": ">=5.6 | ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeArticleBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ],
    "classmap": [
      "src/Drivers/"
    ]
  },

  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeArticleBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_article-",
        "languages_cto": "src/Resources/contao/languages",
        "languages_tx": ".tx"
      }
    }
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "git",
      "url": "git@gitlab.com:metamodels/core.git"
    }
  ]
}
